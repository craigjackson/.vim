set number
syntax on
set expandtab
set ts=4
set shiftwidth=4
set softtabstop=4
set showmode
set showcmd
set ruler<
set autoindent
set smartindent
set hlsearch
set showmatch
set spelllang=en_us
set nocompatible
filetype plugin indent on
set mouse=a
set cursorline
au BufRead,BufNewFile *.svg set filetype=xml
au BufRead,BufNewFile *.module set filetype=php
let NERDTreeIgnore = ['\.pyc$']

